# Don't forget to manually enter GIT_AUTHOR_NAME and GIT_AUTHOR_EMAIL 
# in your .zshrc file.

[alias]
  co = checkout
  pr = pull --rebase
  ci = commit -v
  rollback = reset --soft HEAD^
  st = status -sb
  br = branch
  unstage = reset HEAD
  su = submodule update
  sui = submodule update --init
  lol = log --graph --decorate --oneline
  lola = log --graph --decorate --oneline --all
[merge]
  tool = opendiff
[color]
  ui = auto
[mirror]
  summary = true
[core]
  autocrl = true
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
  excludesfile = /Users/james/.gitignore
  editor = /usr/bin/vim
[difftool]
  prompt = false
[push]
  default = upstream
[status]
  submodulesummary = true
[user]
  name = James Herdman
  email = james.herdman@me.com
[credential]
  helper = osxkeychain
[grep]
	lineNumber = true
